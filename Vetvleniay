function getBushPriceIf (bushName :string) :number {
    if (bushName === "сирень") console.log (100);
    else if (bushName === "жасмин") console.log (120);
    else if (bushName === "шиповник") console.log (150);
    else if (bushName === "роза") console.log (250);
    else if (bushName ==="одуванчик") console.log (80);
}

getBushPriceIf('сирень')
getBushPriceIf('жасмин')
getBushPriceIf('шиповник')
getBushPriceIf('роза')
getBushPriceIf('одуванчик')
