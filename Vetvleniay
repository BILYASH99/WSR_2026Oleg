function getBushPriceIf (bushName :string) :number {
    if (bushName === "сирень") console.log (100);
    else if (bushName === "жасмин") console.log (120);
    else if (bushName === "шиповник") console.log (150);
    else if (bushName === "роза") console.log (250);
    else if (bushName ==="одуванчик") console.log (80);
}

getBushPriceIf('сирень')
getBushPriceIf('жасмин')
getBushPriceIf('шиповник')
getBushPriceIf('роза')
getBushPriceIf('одуванчик')

2 способ
function getBushPriceSwitch (bushName: string) :number {
    let price;
    switch (bushName) {
        case "сирень" :
            price = 100;
            console.log(100)
            break;
        case "жасмин" :
            price = 120;
            console.log(120)
            break;
        case "шиповник" :
            price = 150;
            console.log(150)
            break;
        case "роза":
            price = 250;console.log(250)
            break;
        case "одуванчик":
            price = 80;
            console.log(80)
    }
}
console.log(getBushPriceSwitch("сирень"));
console.log(getBushPriceSwitch("жасмин"));
console.log(getBushPriceSwitch("шиповник"));
console.log(getBushPriceSwitch("роза"));
console.log(getBushPriceSwitch("одуванчик"));
